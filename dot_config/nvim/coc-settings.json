{
  "languageserver": {
    "rubocop": {
      "command": "rubocop",
      "args": ["--lsp"],
      "filetypes": ["ruby"]
    },
    "ruby-lsp": {
      "command": "ruby-lsp",
      "initializationOptions": {
        "experimentalFeaturesEnabled": false,
        "enabledFeatures": [
          "codeActions",
          "diagnostics",
          "documentHighlights",
          "documentLink",
          "documentSymbols",
          "foldingRanges",
          "formatting",
          "hover",
          "inlayHint",
          "onTypeFormatting",
          "selectionRanges",
          "semanticHighlighting",
          "completion"
        ],
        "formatter": "auto",
      },
      "filetypes": ["ruby"]
    },
    "sorbet": {
      "command": "srb",
      "args": ["tc", "--typed", "true", "--enable-all-experimental-lsp-features", "--lsp", "--disable-watchman"],
      "filetypes": ["ruby"],
      "rootPatterns": ["sorbet/config"],
      "initializationOptions": {},
      "settings": {}
    }
  },
 "elixir.pathToElixirLS": "~/code/elixir-ls/language_server.sh",
}
